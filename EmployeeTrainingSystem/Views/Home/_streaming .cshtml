<script>
    AOS.init({
        offset: 200,
        duration: 600,
        easing: 'ease-in-sine',
        delay: 300,
        once: 'true',
    });
</script>
<style>

</style>
<link href="~/css/site.css" rel="stylesheet" />
<div data-aos="fade-up" >
